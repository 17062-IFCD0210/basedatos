<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BaseDatos</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Servlet de prueba para insertar un nuevo registro en la tabla test</description>
    <display-name>insertarServlet</display-name>
    <servlet-name>insertarServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatosProfe.controllers.InsertarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>insertarServlet</servlet-name>
    <url-pattern>/insertar</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Elimina registro de la tabla test de la base de datos skalada</description>
    <display-name>eliminarServlet</display-name>
    <servlet-name>eliminarServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatosProfe.controllers.EliminarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>eliminarServlet</servlet-name>
    <url-pattern>/eliminar</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Edita el registro seleccionado de la tabla test de la bbdd skalada</description>
    <display-name>EditarServlet</display-name>
    <servlet-name>EditarServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatosProfe.controllers.EditarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditarServlet</servlet-name>
    <url-pattern>/Editar</url-pattern>
  </servlet-mapping>
</web-app>