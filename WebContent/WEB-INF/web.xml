<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BaseDatos</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Servlet de prueba para insertar un nuevo registro en la tabla test</description>
    <display-name>InsertarServlet</display-name>
    <servlet-name>InsertarServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatos.controllers.InsertarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertarServlet</servlet-name>
    <url-pattern>/insertar</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EliminarServlet</display-name>
    <servlet-name>EliminarServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatos.controllers.EliminarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EliminarServlet</servlet-name>
    <url-pattern>/eliminar</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Listado de la BBDD segun nota</description>
    <display-name>ListadoServlet</display-name>
    <servlet-name>ListadoServlet</servlet-name>
    <servlet-class>com.ipartek.formacion.basedatos.controllers.ListadoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListadoServlet</servlet-name>
    <url-pattern>/lista</url-pattern>
  </servlet-mapping>
</web-app>